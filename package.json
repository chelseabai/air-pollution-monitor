{
  "name": "heroku-deployment",
  "version": "1.0.0",
  "description": "Deploy react frontend and node.js backend to heroku.",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "build": "cd frontend && npm install && npm run build",
    "heroku-postbuild": "npm run install-frontend && npm run build",
    "frontend": "cd frontend && npm start"
  },
  "keywords": [
    "node",
    "heroku",
    "react"
  ],
  "license": "MIT",
  "dependencies": {
    "aqi-us": "^0.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^6.0.12",
    "node-fetch": "^3.0.0",
    "node-schedule": "^2.0.0",
    "nodemon": "^2.0.14"
  }
}
